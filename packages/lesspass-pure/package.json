    "balloon-css": "^1.0.3",
    "bootstrap": "^4.6.0",
    "copy-text-to-clipboard": "^3.0.1",
    "core-js": "^3.9.1",
    "font-awesome": "^4.7.0",
    "jwt-decode": "^3.1.2",
    "lesspass": "^9.0.0",
    "lodash": "^4.17.21",
    "vue": "^2.6.12",
    "vue-polyglot": "^2.0.1",
    "vue-router": "^3.5.1",
    "vuejs-paginate": "^2.1.0",
    "vuex": "^3.6.2",
