{
  "name": "lesspass-pure",
  "version": "5.0.5",
  "description": "pure LessPass component for LessPass with vuejs",
  "license": "GPL-3.0",
  "author": "Guillaume Vincent <guillaume@oslab.fr>",
  "repository": "lesspass/pure",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "precommit": "npm run test:unit && lint-staged",
    "build": "rm -rf dist && cross-env NODE_ENV=production webpack -p",
    "build:i18n": "cd scripts && node buildI18n.js",
    "dev": "webpack-dev-server --inline --hot --host 0.0.0.0",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "ava",
    "test:e2e": "node test/e2e/runner.js"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "awesomplete": "^1.1.2",
    "axios": "^0.16.2",
    "bootstrap": "^4.0.0-beta.2",
    "copy-text-to-clipboard": "^1.0.2",
    "font-awesome": "^4.7.0",
    "hint.css": "^2.5.0",
    "jwt-decode": "^2.2.0",
    "lesspass": "^6.0.0",
    "lodash.debounce": "^4.0.8",
    "vue": "^2.5.2",
    "vue-polyglot": "^0.2.3",
    "vue-router": "^2.8.1",
    "vuejs-paginate": "^1.2.0",
    "vuex": "^2.5.0",
    "vuex-persistedstate": "^2.0.0",
    "vuex-router-sync": "^4.3.2"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "chromedriver": "^2.31.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "nightwatch": "^0.9.16",
    "nock": "^9.0.14",
    "node-sass": "^4.5.3",
    "prettier": "^1.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "timekeeper": "^1.0.0",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-polyglot-utils": "^0.1.1",
    "vue-template-compiler": "^2.5.2",
    "walk": "^2.3.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit",
    "files": [
      "test/unit/**/*.js"
    ]
  },
  "lint-staged": {
    "{src,test,scripts}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
